{
  "manifest_version": 3,
  "name": "Strava Kudos Clicker",
  "version": "1.0.1",
  "description": "Automatically gives kudos to all posts on strava dashboard",
  "content_scripts": [
    {
      "matches": ["https://www.strava.com/dashboard*"],
      "js": ["contentScript.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
